@{
  ViewData["Title"] = "Registration Page";
}
<h1>Register Your Git Profile</h1>
<hr />
<div class="col-md-6">
  <form action="/home/Uploadprofile">
    <label for="firstname" class="form-label">First Name</label><br>
    <input type="text" id="firstname" name="firstname" class="form-control form-control-lg" value="Durgesh"
      placeholder="Enter First Name" required><br>

    <label for="lastname" class="form-label">Last name</label><br>
    <input type="text" id="lastname" name="lastname" class="form-control form-control-lg" value="Wagh"
      placeholder="Enter last Name" required><br>

    <label for="username" class="form-label">GitHub Username</label><br>
    <input type="text" id="username" name="username" class="form-control form-control-lg" value="waghdurgesh"
      placeholder="Enter Git UserID" required><br>

    <label for="email" class="form-label">Email ID</label><br>
    <input type="email" id="email" name="email" class="form-control form-control-lg" value="durgeshwagh@gmail.com"
      placeholder="Enter Email Id" required><br>

    <label for="profile" class="form-label">Profile Url</label><br>
    <input type="url" id="profileurl" name="profileurl" class="form-control form-control-lg" value="https://github.com/waghdurgesh"
      placeholder="Enter Profile URL" required><br><br>
    <button type="submit" class="btn btn-warning">Submit Profile</button>
  </form>
</div>
